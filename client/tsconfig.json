{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "es5",
    "moduleResolution": "bundler",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "jest.config.js",
    "jest.setup.js",
    "*.config.js",
    "*.config.mjs"
  ],
  "exclude": ["node_modules"]
}
